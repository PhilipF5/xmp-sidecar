{
  "name": "xmp-sidecar",
  "version": "0.1.0",
  "description": "Reads and writes XMP sidecar files for image metadata",
  "main": "xmp-sidecar.js",
  "author": "Philip Fulgham <philfulgham@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "12.12.3",
    "@types/xml2js": "0.4.5",
    "xml2js": "0.4.22"
  },
  "devDependencies": {
    "@types/chai": "4.2.4",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "source-map-support": "0.5.16",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.4",
    "webpack": "4.41.2"
  },
  "scripts": {
    "build": "webpack",
    "test": "nyc mocha -r ts-node/register -r source-map-support/register src/**/*.spec.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js"
    ],
    "all": true,
    "reporter": [
      "text",
      "html"
    ]
  }
}
